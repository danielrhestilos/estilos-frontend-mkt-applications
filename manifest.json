{
  "vendor": "estilospe",
  "name": "frontend-applications",
  "version": "0.0.63",
  "title": "Aplicaciones Frontend - Estilos",
  "description": "Aplicaciones Frontend - Estilos",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x"
  },
  "dependencies": {
    "vtex.css-handles": "0.x",
    "vtex.render-runtime": "8.x",
    "vtex.store-image": "0.x",
    "vtex.styleguide": "9.x",
    "vtex.search-graphql": "0.x",
    "vtex.product-context": "0.x",
    "vtex.product-summary-context": "0.x",
    "vtex.order-manager": "0.x",
    "vtex.format-currency": "0.x",
    "vtex.rich-text": "0.x",
    "vtex.order-items": "0.x",
    "vtex.search-page-context": "0.x",
    "vtex.slider": "0.x",
    "vtex.pixel-manager": "1.x",
    "vtex.order-placed": "2.x",
    "vtex.device-detector": "0.x",
    "vtex.store-graphql": "2.x",
    "vtex.add-to-cart-button": "0.x",
    "vtex.checkout-resources": "0.x",
    "vtex.modal-layout": "0.x",
    "vtex.slider-layout": "0.x",
    "vtex.session-client": "1.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "myvtex.com",
        "path": "/_v/graphql/private/v1"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.flixmedia.com",
        "path": "/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/*"
      }
    }
  ],
  "credentialType": "absolute",
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
